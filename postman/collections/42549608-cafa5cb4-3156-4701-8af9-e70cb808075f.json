							"pm.test('Response has the required fields', function () {",
							"    const responseData = pm.response.json();",
							"    pm.expect(responseData).to.be.an('array');",
							"    responseData.forEach(function (shortlist) {",
							"        pm.expect(shortlist).to.have.property('id');",
